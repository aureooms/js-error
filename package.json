{
  "name": "@aureooms/js-error",
  "description": "Error objects for JavaScript",
  "version": "4.0.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "sources": [
      "src/**/*"
    ],
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-error/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "ava": "2.4.0",
    "coveralls": "3.0.13",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "5.2.1",
    "nyc": "14.1.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "http://aureooms.github.io/js-error/",
  "keywords": [
    "Error",
    "IndexError",
    "KeyError",
    "NotImplementedError",
    "StopIteration",
    "SyntaxError",
    "TypeError",
    "ValueError",
    "error",
    "exception",
    "javascript",
    "js"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-error.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepublish": "npm run build",
    "release": "np",
    "test": "ava --concurrency 8"
  }
}
